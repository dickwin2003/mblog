{
  "name": "mblog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "pages:build": "npm install --no-package-lock && npm install --no-package-lock @remix-run/dev esbuild@0.17.19 && npx remix build && mkdir -p ./public/build && cp -r ./public/* ./public/build/",
    "pages:deploy": "npm run pages:build && wrangler pages deploy ./public/build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@libsql/client": "^0.5.3",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@remix-run/dev": "^2.3.1",
    "esbuild": "0.17.19",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@remix-run/eslint-config": "^2.3.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.38.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "wrangler": "^3.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
