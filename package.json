{
  "name": "mblog",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "wrangler pages dev ./public",
    "deploy": "npm install && remix build && wrangler pages deploy ./public --project-name mblog --commit-dirty=true",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "crypto-browserify": "^3.12.0",
    "path-browserify": "^1.0.1",
    "os-browserify": "^0.3.0",
    "querystring-es3": "^0.2.1",
    "buffer": "^6.0.3",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.38.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "wrangler": "^3.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browser": {
    "crypto": "crypto-browserify",
    "stream": "stream-browserify",
    "path": "path-browserify",
    "os": "os-browserify",
    "buffer": "buffer",
    "events": "events",
    "fs": false,
    "net": false,
    "tls": false,
    "zlib": false
  }
}
